    <script>
        window.onload = function loadDate() {
            let date = new Date(),
                day = date.getDate(),
                month = date.getMonth() + 1,
                year = date.getFullYear();

            hower = date.getHours();
            Minit = date.getMinutes();
            sec = date.getSeconds();

            //if (month < 10) month = "0" + month;
            //if (day < 10) day = "0" + day;
            if (hower < 10) hower = "0" + hower;
            if (Minit < 10) Minit = "0" + Minit;
            if (sec < 10) sec = "0" + sec;

            const todayDate = `${hower}:${Minit}`;
            document.getElementById("BATS").defaultValue = todayDate;
            document.getElementById("BATE").defaultValue = todayDate;
            document.getElementById("BBTS").defaultValue = todayDate;
            document.getElementById("BBTE").defaultValue = todayDate;
            document.getElementById("BCTS").defaultValue = todayDate;
            document.getElementById("BCTE").defaultValue = todayDate;
        };


    </script>

