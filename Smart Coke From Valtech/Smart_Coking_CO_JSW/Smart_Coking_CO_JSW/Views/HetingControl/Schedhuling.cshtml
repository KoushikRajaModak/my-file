                    <input type="time" id="BBTE" class="xB1T" />
                    <input type="submit" value="Save" id="btnB1T" class="xsetbtn" onclick="B_Relode()" />

                    @* C *@

                    <label for="BCTS">Break Time C_Shift:</label>
                    <input type="time" id="BCTS" class="xB1T" />
                    <input type="time" id="BCTE" class="xB1T" />
                    <input type="submit" value="Save" id="btnB1T" class="xsetbtn" onclick="C_Relode()" />




                </div>


