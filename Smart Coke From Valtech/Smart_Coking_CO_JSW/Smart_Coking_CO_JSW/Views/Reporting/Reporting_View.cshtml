<body>
    @{
        string TodayN = DateTime.Now.ToString("yyyy-MM-dd HH:mm");
        string TodayO = DateTime.Now.ToString("yyyy-MM-dd");
        string YesterdayN = DateTime.Now.AddDays(-5).ToString("yyyy-MM-dd HH:mm");
    }
    <div class="xcointainer3">
        <div class="xtopcontainer">
            <div class="xtopsub1">

                @*@ Production @*@
                <div class="gbtn2">
                    <div class="tagcointainer">
                        <p class="tag"> Production Report</p>
                        <i class="fa-solid fa-magnifying-glass" aria-hidden="true"></i>
                    </div>
                    <label for="Yesterday_Production" class="label1">From</label>
                    <input type="datetime-local" class="datesel" value="@YesterdayN" id="Yesterday_Production" />
                    <label for="Today_Production" class="label1">To</label>
                    <input type="datetime-local" class="datesel1" value="@TodayN" id="Today_Production">
                    <input type="button" value="Load" class="gibtn" id="Calorific_Value" onclick="btn_Production()" />
                </div>

                @*@Daily Report@*@
                <div class="gbtn2">
                    <div class="tagcointainer">
                        <p class="tag">Daily Report</p>
                        <i class="fa-solid fa-magnifying-glass" aria-hidden="true"></i>
                    </div>
                    <label for="Yesterday_DailyR" class="label1">Date </label>
                    <input type="date" class="datesel" value="@TodayO" id="Yesterday_DailyR" />
                    @*<label for="Today_DailyR">To</label>
                        <input type="datetime-local" class="datesel1" value="@TodayN" id="Today_DailyR" />*@
                    <input type="button" value="Load" class="gibtn2" id="Calorific_Value" onclick="btn_DailyR()" />
                </div>
                @*@Maintenance@*@
                <div class="gbtn2">
                    <div class="tagcointainer">
                        <p class="tag">Maintenance Report</p>
                        <i class="fa-solid fa-magnifying-glass" aria-hidden="true"></i>
                    </div>
                    <label for="Yesterday_Maintenance" class="label1">From</label>
                    <input type="datetime-local" class="datesel" value="@YesterdayN" id="Yesterday_Maintenance" />
                    <label for="Today_Maintenance" class="label1">To</label>
                    <input type="datetime-local" class="datesel1" value="@TodayN" id="Today_Maintenance" />
                    <input type="button" value="Load" class="gibtn" id="Calorific_Value" onclick="btn_Maintenance()" />
                </div>
                @*@Control Flue@*@
                <div class="gbtn2">
                    <div class="tagcointainer">
                        <p class="tag">Control Flue Report</p>
                        <i class="fa-solid fa-magnifying-glass" aria-hidden="true"></i>
                    </div>
                    <label for="Yesterday_CFlue" class="label1">From</label>
                    <input type="datetime-local" class="datesel" value="@YesterdayN" id="Yesterday_CFlue" />
                    <label for="Today_CFlue" class="label1">To</label>
                    <input type="datetime-local" class="datesel1" value="@TodayN" id="Today_CFlue" />
                    <input type="button" value="Load" class="gibtn" id="Calorific_Value" onclick="btn_CFlue()" />
                </div>
                @*@NPT@*@
                <div class="gbtn2">
                    <div class="tagcointainer">
                        <p class="tag">NPT Report</p>
                        <i class="fa-solid fa-magnifying-glass" aria-hidden="true"></i>
                    </div>
                    <label for="Yesterday_NPT" class="label1">From</label>
                    <input type="datetime-local" class="datesel" value="@YesterdayN" id="Yesterday_NPT" />
                    <label for="Today_NPT" class="label1">To</label>
                    <input type="datetime-local" class="datesel1" value="@TodayN" id="Today_NPT" />
                    <input type="button" value="Load" class="gibtn" id="Calorific_Value" onclick="btn_NPT()" />
                </div>
                @*@Charging / Pushing@*@
                <div class="gbtn2">
                    <div class="tagcointainer">
                        <p class="tag">Charging & Pushing</p>
                        <i class="fa-solid fa-magnifying-glass" aria-hidden="true"></i>
                    </div>
                    <label for="M_YesterdayN" class="label1">From</label>
                    <input type="datetime-local" class="datesel" value="@YesterdayN" id="M_YesterdayN" />
                    <label for="M_TodayN" class="label1">To</label>
                    <input type="datetime-local" class="datesel1" value="@TodayN" id="M_TodayN" />
                    <input type="button" value="Load" class="gibtn" id="Calorific_Value" onclick="btnSearch()" />
                </div>
            </div>
            @*tableView*@
            <div class="xtopsub2" id="Table_Views_P" hidden>
                <div class="bottom-title captionx">Production Report</div>
                <table class="table align-items-center justify-content-between" style="width:90%;" id="DataTables_P">
                    <thead>

                    </thead>
                </table>
            </div>
            <div class="xtopsub2" id="Table_Views_D" hidden>
